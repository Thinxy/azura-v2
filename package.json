{
  "name": "@atosjs/azura",
  "description": "Azura is a simple and lightweight web framework for Node.js",
  "version": "2.1.3",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.js",
    "types": "./dist/@types/index.d.ts"
  },
  "files": [
    "dist",
    "dist/@types"
  ],
  "scripts": {
    "start": "node .",
    "test": "ts-node test/index.test.ts",
    "dev": "ts-node -r tsconfig-paths/register --transpiler sucrase/ts-node-plugin src/index.ts",
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Thinxy/azura-v2.git",
    "directory": "/"
  },
  "bugs": {
    "url": "https://github.com/Thinxy/azura-v2/issues"
  },
  "homepage": "https://github.com/Thinxy/azura-v2#readme",
  "keywords": [
    "web",
    "framework",
    "api",
    "node"
  ],
  "author": "Rabi & Wyntch",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.13.10",
    "esbuild": "^0.25.0",
    "typescript": "^5.8.2",
    "tscpaths": "^0.0.9"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "figures": "3.2.0",
    "glob": "^11.0.1",
    
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
