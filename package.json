{
  "name": "@atosjs/azura",
  "description": "Azura is a simple and lightweight web framework for Node.js",
  "version": "2.0.2",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist",
    "dist/@types"
  ],
  "scripts": {
    "start": "node .",
    "test": "ts-node test/index.test.ts",
    "dev": "ts-node -r tsconfig-paths/register --transpiler sucrase/ts-node-plugin src/index.ts",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Thinxy/azura-v2.git",
    "directory": "/"
  },
  "bugs": {
    "url": "https://github.com/Thinxy/azura-v2/issues"
  },
  "homepage": "https://github.com/Thinxy/azura-v2#readme",
  "keywords": [
    "web",
    "framework",
    "api",
    "node"
  ],
  "author": "Rabi & Wyntch",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.13.0",
    "rollup": "^3.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "figures": "3.2.0",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
