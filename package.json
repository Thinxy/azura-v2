{
  "name": "@atosjs/azura",
  "description": "Azura is a simple and lightweight web framework for Node.js",
  "version": "2.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node .",
    "test": "ts-node test/index.test.ts",
    "dev": "ts-node -r tsconfig-paths/register --transpiler sucrase/ts-node-plugin src/index.ts",
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Thinxy/azura-v2.git",
    "directory": "/"
  },
  "bugs": {
    "url": "https://github.com/Thinxy/azura-v2/issues"
  },
  "homepage": "https://github.com/Thinxy/azura-v2#readme",
  "keywords": [
    "web",
    "framework",
    "api",
    "node"
  ],
  "author": "Rabi & Wyntch",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.13.0",
    "sucrase": "^3.34.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "@types/bun": "latest"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "figures": "3.2.0",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
